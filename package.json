{
  "name": "flagpole-api",
  "version": "1.0.0",
  "description": "FlagPole Api",
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "seed": "npx ts-node scripts/seeds/seed.ts",
    "seed:org": "npx ts-node scripts/seeds/organization.seed.ts",
    "seed:users": "npx ts-node scripts/seeds/user.seed.ts",
    "seed:projects": "npx ts-node scripts/seeds/project.seed.ts",
    "seed:flags": "npx ts-node scripts/seeds/feature-flag.seed.ts",
    "seed:indexes": "npx ts-node scripts/seeds/indexes.seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/flagpole-corp/flagpole-api.git"
  },
  "author": "FlagPole Corp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flagpole-corp/flagpole-api/issues"
  },
  "homepage": "https://github.com/flagpole-corp/flagpole-api#readme",
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@nestjs/common": "^10.4.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.8",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/websockets": "^10.4.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jest": "^29.7.0",
    "mongoose": "^7.8.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.4.15",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.9.4",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "mongodb": "^6.11.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
